{
  "Import": {
    "prefix": ["imp"],
    "body": [
      "import $1"
    ],
    "description": "Import from module"
  },
  "FromImport": {
    "prefix": ["fimp"],
    "body": [
      "from $1 import $2"
    ],
    "description": "From module import"
  },
  "SetTrace": {
    "prefix": ["st"],
    "body": [
      "import pdb; pdb.set_trace()"
    ],
    "description": "Set trace for debugging"
  },
  "LogInfo": {
    "prefix": ["li"],
    "body": [
      "LOGGER.info(f\"$1: {$1}\")"
    ],
    "description": "Log info"
  },
  "LogDebug": {
    "prefix": ["ld"],
    "body": [
      "LOGGER.debug(f\"$1: {$1}\")"
    ],
    "description": "Log debug"
  },
  "LogWarning": {
    "prefix": ["lw"],
    "body": [
      "LOGGER.warning(f\"$1: {$1}\")"
    ],
    "description": "Log warning"
  },
  "LogError": {
    "prefix": ["le"],
    "body": [
      "LOGGER.error(f\"$1: {$1}\")"
    ],
    "description": "Log error"
  },
  "Test": {
    "prefix": ["test_"],
    "body": [
      "def test_$1($2):",
      "    \"\"\"",
      "    $3",
      "    \"\"\"",
      "    # arrange",
      "    $4",
      "    # act",
      "    # assert"
    ],
    "description": "Log error"
  }
}
