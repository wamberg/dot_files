{
  "Const": {
    "prefix": ["c="],
    "body": [
      "const $1 = $2;"
    ],
    "description": "Declare const"
  },
  "Let": {
    "prefix": ["l="],
    "body": [
      "let $1 = $2;"
    ],
    "description": "Declare variable"
  },
  "AnonArrowFunction": {
    "prefix": ["=>"],
    "body": [
      "($1) => $2;"
    ],
    "description": "Anonymous function"
  },
  "NamedArrowFunction": {
    "prefix": ["c=>"],
    "body": [
      "const $1 = ($2) => $3;"
    ],
    "description": "Declare variable"
  },
  "ConsoleLog": {
    "prefix": ["cl"],
    "body": [
      "console.log(`$1: \\${$1}`);"
    ],
    "description": "Console log"
  },
  "ConsoleLogJSON": {
    "prefix": ["clj"],
    "body": [
      "console.log(`$1: \\${JSON.stringify($1)}`);"
    ],
    "description": "Console log as JSON"
  }
}
